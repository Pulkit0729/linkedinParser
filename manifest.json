{
  "manifest_version": 3,
  "name": "LinkedIn Parser",
  "version": "1.0.0",
  "description": "A basic extension",
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "scripting", "storage"],
  "host_permissions": ["https://*.linkedin.com/*"]
}
